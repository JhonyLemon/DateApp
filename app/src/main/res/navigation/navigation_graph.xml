    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph"
    app:startDestination="@id/swipeFragment">

    <fragment
        android:id="@+id/chatFragment"
        android:name="com.example.dateapp.fragments.ChatFragment"
        android:label="fragment_chat"
        tools:layout="@layout/fragment_chat" />
    <fragment
        android:id="@+id/swipeFragment"
        android:name="com.example.dateapp.fragments.SwipeFragment"
        android:label="fragment_swipe"
        tools:layout="@layout/fragment_swipe" />
    <fragment
        android:id="@+id/likedFragment"
        android:name="com.example.dateapp.fragments.LikedFragment"
        android:label="fragment_liked"
        tools:layout="@layout/fragment_liked" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.dateapp.ProfileActivity"
        android:label="fragment_profile"
        tools:layout="@layout/profile_activity" />
